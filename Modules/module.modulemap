module CGmp [system] {
  header "/usr/local/include/gmp.h"
  link "gmp"
  export *
}
module Clibbsd [system] {
  header "bsd.h"
  link "bsd"
  export *
}
module Clibpng [system] {
	header "/usr/local/include/png.h"
	link "png"
	export *
}
module Clibedit [system] {
	header "/usr/local/opt/libedit/include/editline/readline.h"
	link "curses"
	link "edit"
	export *
}
